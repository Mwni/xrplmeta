{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "Metadata & Marketdata Storage",
	"type": "object",
	"properties": {
		"accounts": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/Account"
			}
		},
		"issuerMetas": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/IssuerMeta"
			}
		},
		"currencies": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/Currency"
			}
		},
		"tokens": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/Token"
			}
		},
		"tokenMetas": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/TokenMeta"
			}
		},
		"tokenExchanges": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/TokenExchange"
			}
		},
		"tokenSupply": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/TokenSupply"
			}
		},
		"tokenTrustlines": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/TokenTrustlines"
			}
		},
		"tokenHolders": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/TokenHolders"
			}
		},
		"tokenWhales": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/TokenWhale"
			}
		},
		"tokenWhaleBalances": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/TokenWhaleBalance"
			}
		},
		"tokenBooks": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/TokenBook"
			}
		},
		"tokenBookOffers": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/TokenBookOffer"
			}
		}
	},
	"definitions": {
		"Account": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"address": {
					"type": "string",
					"format": "xrpl/address"
				}
			},
			"required": [
				"address"
			],
			"unique": [
				"address"
			]
		},
		"IssuerMeta": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"issuer": {
					"$ref": "#/definitions/Account"
				},
				"key": {
					"type": "string"
				},
				"value": {
					"type": "any"
				},
				"source": {
					"type": "string"
				}
			},
			"required": [
				"issuer",
				"key",
				"source"
			],
			"unique": [
				["issuer", "key", "source"]
			]
		},
		"Currency": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"code": {
					"type": "string",
					"format": "xrpl/currency"
				}
			},
			"required": [
				"code"
			],
			"unique": [
				"code"
			]
		},
		"Token": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"currency": {
					"$ref": "#/definitions/Currency"
				},
				"issuer": {
					"$ref": "#/definitions/Account"
				}
			},
			"required": [
				"currency",
				"issuer"
			],
			"unique": [
				["currency", "issuer"]
			]
		},
		"TokenMeta": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"token": {
					"$ref": "#/definitions/Token"
				},
				"key": {
					"type": "string"
				},
				"value": {
					"type": "any"
				},
				"source": {
					"type": "string"
				}
			},
			"required": [
				"token",
				"key",
				"source"
			],
			"unique": [
				["token", "key", "source"]
			]
		},
		"TokenExchange": {
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"txHash": {
					"type": "string",
					"format": "hex"
				},
				"offerIdx": {
					"type": "integer"
				},
				"ledgerIndex": {
					"type": "integer"
				},
				"taker": {
					"$ref": "#/definitions/Account"
				},
				"takerPaysToken": {
					"$ref": "#/definitions/Token"
				},
				"takerPaysValue": {
					"type": "string",
					"format": "xrpl/xfl"
				},
				"takerGetsToken": {
					"$ref": "#/definitions/Token"
				},
				"takerGetsValue": {
					"type": "string",
					"format": "xrpl/xfl"
				}
			},
			"required": [
				"txHash",
				"offerIdx",
				"ledgerIndex",
				"taker",
				"takerPaysValue",
				"takerGetsValue"
			],
			"unique": [
				["txHash", "offerIdx"]
			]
		},
		"TokenSupply": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"token": {
					"$ref": "#/definitions/Token"
				},
				"ledgerIndex": {
					"type": "integer"
				},
				"value": {
					"type": "string",
					"format": "xrpl/xfl"
				}
			},
			"required": [
				"token",
				"ledgerIndex",
				"value"
			],
			"unique": [
				["token", "ledgerIndex"]
			]
		},
		"TokenTrustlines": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"token": {
					"$ref": "#/definitions/Token"
				},
				"ledgerIndex": {
					"type": "integer"
				},
				"value": {
					"type": "integer"
				}
			},
			"required": [
				"token",
				"ledgerIndex",
				"value"
			],
			"unique": [
				["token", "ledgerIndex"]
			]
		},
		"TokenHolders": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"token": {
					"$ref": "#/definitions/Token"
				},
				"ledgerIndex": {
					"type": "integer"
				},
				"value": {
					"type": "integer"
				}
			},
			"required": [
				"token",
				"ledgerIndex",
				"value"
			],
			"unique": [
				["token", "ledgerIndex"]
			]
		},
		"TokenWhale": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"token": {
					"$ref": "#/definitions/Token"
				},
				"account": {
					"$ref": "#/definitions/Account"
				},
				"balances": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/TokenWhaleBalance"
					}
				}
			},
			"required": [
				"token",
				"account"
			],
			"unique": [
				["token", "account"]
			]
		},
		"TokenWhaleBalance": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"whale": {
					"$ref": "#/definitions/TokenWhale"
				},
				"ledgerIndex": {
					"type": "integer"
				},
				"value": {
					"type": "string",
					"format": "xrpl/xfl"
				}
			},
			"required": [
				"whale",
				"ledgerIndex",
				"value"
			],
			"unique": [
				["whale", "ledgerIndex"]
			]
		},
		"TokenBook": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"takerPaysToken": {
					"$ref": "#/definitions/Token"
				},
				"takerGetsToken": {
					"$ref": "#/definitions/Token"
				},
				"offers": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/TokenBookOffer"
					}
				}
			},
			"required": [],
			"unique": [
				["takerPaysToken", "takerGetsToken"]
			]
		},
		"TokenBookOffer": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"book": {
					"$ref": "#/definitions/TokenBook"
				},
				"account": {
					"$ref": "#/definitions/Account"
				},
				"sequence": {
					"type": "integer"
				},
				"startLedgerIndex": {
					"type": "integer"
				},
				"endLedgerIndex": {
					"type": "integer"
				},
				"takerPaysValue": {
					"type": "string",
					"format": "xrpl/xfl"
				},
				"takerGetsValue": {
					"type": "string",
					"format": "xrpl/xfl"
				}
			},
			"required": [
				"book",
				"offerId",
				"startLedgerIndex",
				"takerPaysValue",
				"takerGetsValue"
			],
			"unique": [
				["account", "sequence"]
			]
		}
	}
}