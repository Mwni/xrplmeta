datasource db {
	provider = "sqlite"
	url      = env("META_FILE")
}

generator client {
	provider = "prisma-client-js"
	output = "clients/meta"
	previewFeatures = ["interactiveTransactions"]
}

model Issuer {
	id Int @id @default(autoincrement())
	address String @unique

	tokens Token[]
}

model Token {
	id Int @id @default(autoincrement())
	currency String
	issuerId Int
	inception DateTime?

	issuer Issuer @relation(fields: [issuerId], references: [id])
}