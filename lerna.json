{
  "version": "1.0.0-alpha",
  "npmClient": "npm",
  "command": {
    "publish": {
      "ignoreChanges": [
        "ignored-file",
        "*.md"
      ]
    },
    "bootstrap": {
      "npmClientArgs": [
        "--no-package-lock"
      ]
    }
  },
  "packages": [
    "server",
    "toml",
    "crawler",
    "db",
    "log",
    "migration",
    "repo",
    "utils",
    "release"
  ]
}